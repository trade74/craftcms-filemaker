{
    "name": "craftyfm/craftcms-filemaker",
    "description": "A craft CMS plugin to simplify connection to the Filemaker Data API. Connect to a filemaker database and use a button from Filemaker to push data into Craft CMS. This plugin also using webhooks to push dat into Filemaker's Data API",
    "type": "craft-plugin",
    "license": "proprietary",
    "support": {
        "email": "stuart@x2network.net"
    },
    "version": "1.0.0",
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.5.0"
    },
    "require-dev": {
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "craftyfm\\craftcms-filemaker\\": "src/"
        },
        "exclude-from-classmap": [
            "vendor/craftcms/feed-me/src/helpers/AssetHelper.php"
        ],
        "files": [
            "vendor/craftyfm/craftcms-filemaker/src/overrides/AssetHelper.php"
        ]
    },
    "extra": {
        "handle": "filemaker",
        "name": "filemaker",
        "developer": "Stuart Russell",
        "documentationUrl": ""
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

